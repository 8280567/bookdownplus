### mindr
remove.packages('mindr')
devtools::install_github("pzhaonet/mindr")

library(mindr)
md2mm()
md2mm(remove_curly_bracket = TRUE)
mm2md()
outline(remove_curly_bracket = TRUE)
outline()

### postr
remove.packages('postr')
devtools::install_github("pzhaonet/postr")
postr::postr()
postr::postr(theme = 'night')
postr::postr(empty = TRUE)
postr::postr_del()

### bookdownplus
remove.packages('bookdownplus')
library('bookdownplus')
bookdownplus()

### mf
remove.packages('mf')
devtools::install_github('pzhaonet/mf')
mf::mf_bib()
example(mf::mf_bib())
